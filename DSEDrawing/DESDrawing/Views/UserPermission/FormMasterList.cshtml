@model List<DESDrawing.Models.FormMasterModel>
@{
    ViewBag.Title = "FormMasterList";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<div class="page-wrapper">
    @{
        string Code = (string)TempData["code"];
        string msg = (string)TempData["Msg"];
        if (!string.IsNullOrEmpty(Code))
        {
            if (Code == "1")
            {
                <div class="alert alert-success msgdiv">
                    <strong>Success!</strong> @msg
                    <button type="button" class="close" data-dismiss="alert" aria-label="Close"> <span aria-hidden="true">&times;</span> </button>
                </div>
            }
            else
            {
                <div class="alert alert-danger msgdiv">
                    <strong>Error!</strong> @msg
                    <button type="button" class="close" data-dismiss="alert" aria-label="Close"> <span aria-hidden="true">&times;</span> </button>
                </div>
            }
        }
    }
    <link href="~/assets/css/datatables.min.css" rel="stylesheet" />
    <script src="~/assets/js/core/datatables.min.js"></script>




    <div class="row page-titles">
        <div class="col-md-6 align-self-center">
            <h3 class="text-themecolor font-20 clearfix">Form Master List</h3>
            <nav aria-label="breadcrumb" class="float-left">
                <ol class="breadcrumb">
                    <li class="breadcrumb-item"><a href="#-">Admin</a></li>
                    <li class="breadcrumb-item"><a href="#-">User Permission</a></li>
                    <li class="breadcrumb-item active" aria-current="page">Form Master List</li>
                </ol>
            </nav>
        </div>        
        <div class="pbutton">
            <div class="right text-right">
                @*@if (PermissionManager.IsActionPermit("Form Master", ViewOptions.FormSave))
        {*@
                <a href="/UserPermission/FormMaster" id="btnAdd" class="btn btn-success">
                    <i class="fa fa-plus" aria-hidden="true"></i> Form Master
                </a>
                @*}*@
            </div>
        </div>
    </div>
    <div id="no-more-tables">
        <div class="row">
            <div class="col-lg-12">

                <div class="box">
                    <div class="box-body">
                        <div class="card">
                            <div class="card-body">
                                <div class="row">
                                    <div class="col-md-9">
                                    </div>
                                </div>

                                <div class="table-responsive">
                                    <table id="myTable" class="table table-hover table-striped mb-0 datatable">

                                        <thead class="table-primary">
                                            <tr class="hdr">
                                                <th align="left" scope="col">S. No.</th>
                                                <th align="left" scope="col">Form Type</th>
                                                <th align="left" scope="col">Form Name</th>
                                                <th align="left" scope="col">Action</th>
                                            </tr>

                                        </thead>

                                        <tbody>

                                            @if (Model != null)
                                            {
                                                int SNO = 0;
                                                foreach (var item in Model)
                                                {
                                                    <tr>
                                                        <td>@(SNO = SNO + 1)</td>
                                                        <td>@item.FormTypeName</td>
                                                        <td>@item.FormName</td>
                                                        <td>
                                                            @*@if (PermissionManager.IsActionPermit("Form Master", ViewOptions.FormUpdate))
                                                                {*@
                                                            <a href="/UserPermission/FormMasterFill?id=@item.PK_FormId" title="Edit" onclick="return confirm ('Are you sure to Edit?');" class="btn btn-primary btn-sm"><i class="fa fa-edit"></i></a>
                                                            @*}
                                                                @if (PermissionManager.IsActionPermit("Form Master", ViewOptions.FormDelete))
                                                                {*@
                                                            <a href="/UserPermission/FormMasterDelete?id=@item.PK_FormId" title="Delete" onclick="return confirm ('Are you sure to Delete?');" class="btn btn-danger btn-sm"><i class="fa fa-trash"></i></a>
                                                            @*}*@

                                                        </td>
                                                    </tr>
                                                }
                                            }
                                        </tbody>
                                    </table>
                                </div><!-- /.box-body -->
                            </div>
                        </div><!-- /.box -->



                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
















<script>
    $("#tblFormMaster").DataTable({
        "lengthMenu": [[20, 50, 100, -1], [20, 50, 100, "All"]]
    });
</script>






